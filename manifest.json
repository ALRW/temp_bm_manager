{
  "manifest_version": 2,
  "name": "Temporary Bookmark Manager",
  "version": "1.0",
  "description": "An extension for short-life bookmark management",
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Temporary Bookmark Manager",
    "default_popup": "popup.html"
  },
  "author": "Andrew Werner",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "devtools_page": "devtools.html",
  "externally_connectable": {
    "ids": ["*"]
  },
  "homepage_url": "https://github.com/ALRW/temp_bm_manager",
  "offline_enabled": true,
  "permissions": ["tabs", "activeTab", "bookmarks"],
  "short_name": "TBM"
}
