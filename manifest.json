{
  "manifest_version": 2,
  "name": "Temporary Bookmark Manager",
  "version": "versionString",
  "default_locale": "en",
  "description": "An extension for short-life bookmark management",
  "icons": {
    "48": "icon48.png",
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Temporary Bookmark Manager",
    "default_popup": "popup.html"
  },

  "author": "Andrew Werner",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "chrome_ui_overrides": {
    "bookmarks_ui": {
      "remove_bookmark_shortcut": true,
      "remove_button": true
    }
  },
  "commands": {},
  "content_scripts": [{
    "js":["content.js"]
    }],
  "devtools_page": "devtools.html",
  "externally_connectable": {
    "ids": ["*"]
  },
  "homepage_url": "https://github.com/ALRW/temp_bm_manager",
  "offline_enabled": true,
  "permissions": ["tabs", "activeTab", "bookmarks"],
  "short_name": "TBM",
}
